#include <iostream>

using namespace std;

main(){
	float V1,V2,V3,media;
	cout<<"Digite o primeiro valor:"<<endl;
	 cin>>V1;
	cout<<"Digite o segundo valor"<<endl;
	 cin>>V2;
	cout<<"Digite o terceiro valor"<<endl;
	 cin>>V3;
	  media=(V1+V2+V3)/3;
	cout<<"Sua media eh: "<<media;
	system("pause");
}
